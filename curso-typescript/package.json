{
  "name": "curso-typescript",
  "version": "1.0.0",
  "description": "aprendiendo y desarrollado typescript TS",
  "main": "index.js",
  "scripts": {
    "tsNode": "cd src && ts-node index.ts",
    "start": "nodemon",
    "linting": "eslint",
